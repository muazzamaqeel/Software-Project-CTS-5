QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    color: black;
    border: 4px solid white;
}

/* Change the up arrow when the QComboBox is opened */
QComboBox::up-arrow:open {
    image: url(:/down-arrow.png); /* Arrow-down icon when the combo box is opened */
    width: 20px;
    height: 20px;
    border-radius: 4px;
    background-repeat: no-repeat;
    background-position: center;
    border: none;
}

/* Change the down arrow when the QComboBox is closed */
QComboBox::down-arrow:closed {
    image: url(:/up-arrow.png); /* Arrow-up icon when the combo box is closed */
    width: 20px;
    height: 20px;
    border-radius: 4px;
    background-repeat: no-repeat;
    background-position: center;
    border: none;
}

/* QComboBox popup menu */
QComboBox QAbstractItemView {
    background-color: white;
    color: black;
    border: 1px solid white;
    border-radius: 4px;
    bottom: -1px; /* Position the dropdown box just under the combo box */
    margin-top: 1px;
}

QComboBox QAbstractItemView::item {
    height: 25px;
    padding: 3px 10px;
    border-radius: 4px;
}

QComboBox QAbstractItemView::item:selected {
    background-color: #D5FEFF;
    color: black;
    border-radius: 4px;
}
