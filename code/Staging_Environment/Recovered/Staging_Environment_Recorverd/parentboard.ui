    padding: 3px;
    border-bottom: 1px solid transparent; /* Transparent border for item bottoms */
}

QTableWidget::item:selected {
    background-color: #b3d1ff; /* A medium blue for selected item */
}

QHeaderView::section {
    background-color: #00165a; /* A darker blue for header sections */
    color: white;
    padding: 1px;
    border-top: none;
}

