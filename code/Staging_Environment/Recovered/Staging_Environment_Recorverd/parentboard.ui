              <string notr="true">/* QTreeWidget Stylesheet */

/* QTreeWidget background and basic styles */
QTreeWidget {
    background-color: #f0f0f0; /* Light gray background */
    border: none; /* Remove border */
    font: 14pt &quot;Arial&quot;;
}

/* Top-level items */
QTreeWidget::item {
    padding: 10px 15px; /* Increase padding for better spacing */
    background-color: #ffffff; /* White background */
    color: #333333; /* Dark gray text color */
    border: 1px solid #d1d1d1; /* Add a subtle border */
}

QTreeWidget::item:hover {
    background-color: #f2f2f2; /* Slightly lighter background on hover */
}

QTreeWidget::item:selected {
    background-color: #86ff98; /* Light green background for selected items */
    color: #333333; /* Dark gray text color */
    font-weight: bold;
}

/* Subitems (nested items) */
QTreeWidget::item:has-children {
    background-color: #ffffff; /* White background for items with children */
    border: 1px solid #d1d1d1; /* Add a subtle border */
}

QTreeWidget::item:!has-children {
    background-color: #ffffff; /* White background for top-level items without children */
}

QTreeWidget::item:has-children:hover {
    background-color: #ffffff; /* White background on hover for items with children */
}

QTreeWidget::item:!has-children:hover {
    background-color: #ffffff; /* White background on hover for top-level items without children */
}

QTreeWidget::item:has-children:selected {
    background-color: #ffffff; /* White background for selected items with children */
}

QTreeWidget::item:!has-children:selected {
    background-color: #86ff98; /* Light green background for selected top-level items without children */
    font-weight: bold;
}

/* QHeaderView Sections (Individual Columns) */
QHeaderView::section {
   background-color: #22344C;; /* Dark gray background for header sections */
    color: #ffffff; /* White text color for header sections */
    padding: 8px;
    border: none; /* Remove border */
    qproperty-horizontalStretch: 1; /* Stretch the header to fill the entire width */
    qproperty-stretchSection: 1; /* Stretch all sections to fill the remaining space */
}

